panduitCmnAlarmCount OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Number of Active Alarms in system"
    ::= { panduitCmnAlarms 1 }

panduitCmnAlarmSeveritySummary OBJECT-TYPE
    SYNTAX      AlarmSeverityLevel
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The highest level severity of all active alarms"
    ::= { panduitCmnAlarms 2 }

panduitCmnAlarmTable OBJECT-TYPE
    SYNTAX SEQUENCE OF PanduitCmnAlarmEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Table of Active Alarms"
    ::= { panduitCmnAlarms 3 }

panduitCmnAlarmEntry OBJECT-TYPE
    SYNTAX PanduitCmnAlarmEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The entry containing information applicable to an alarm"
    INDEX { panduitCmnAlarmId }
    ::= { panduitCmnAlarmTable 1 }

PanduitCmnAlarmEntry ::= SEQUENCE {
        panduitCmnAlarmId                       Unsigned32,
        panduitCmnAlarmEntity                   AutonomousType,
        panduitCmnAlarmDescr                    AutonomousType,
        panduitCmnAlarmSeverity                 AlarmSeverityLevel,
        panduitCmnAlarmText                     DisplayString,
        panduitCmnAlarmTime                     TimeStamp
    }

panduitCmnAlarmId OBJECT-TYPE
    SYNTAX Unsigned32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION "Unique identifier for an alarm condition"
    ::= { panduitCmnAlarmEntry 1 }

panduitCmnAlarmEntity OBJECT-TYPE
    SYNTAX AutonomousType
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION "A reference to the entity object that the alarm condition is raised against"
    ::= { panduitCmnAlarmEntry 2 }

panduitCmnAlarmDescr OBJECT-TYPE
    SYNTAX AutonomousType
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION "A reference to the alarm description object."
    ::= { panduitCmnAlarmEntry 3 }

panduitCmnAlarmSeverity OBJECT-TYPE
    SYNTAX      AlarmSeverityLevel
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION "Indicates how severely this impacts the operation of the system"
    ::= { panduitCmnAlarmEntry 4 }

panduitCmnAlarmText OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION "Additional text regarding the alarm"
    ::= { panduitCmnAlarmEntry 5 }

panduitCmnAlarmTime OBJECT-TYPE
    SYNTAX TimeStamp
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION "The value of sysUpTime when the alarm condition was detected"
    ::= { panduitCmnAlarmEntry 6 }
