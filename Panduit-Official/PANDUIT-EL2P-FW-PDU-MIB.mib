--
-- Textual Conventions
--
Temperature ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d-1"
    STATUS current
    DESCRIPTION "The values for Temperature in 0.1 units specified in TemperatureUnits"
    SYNTAX      Integer32

Humidity ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d-1"
    STATUS current
    DESCRIPTION "The values for Humidity in 0.1 %RH."
    SYNTAX      Integer32

AnalogThreshold ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d-1"
    STATUS current
    DESCRIPTION "The values for The analog threshold value in 0.1 units. Could be temperature degrees or relative humidity precent."
    SYNTAX      Integer32

ContactState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Contact sensor state."
    SYNTAX      INTEGER { open(1), close(2) }

WaterContactState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Water sensor state."
    SYNTAX      INTEGER { noLeak(1), leak(2) }

TemperatureUnits ::= TEXTUAL-CONVENTION
    STATUS current
    DESCRIPTION "The values for TemperatureUnits"
    SYNTAX      INTEGER { celsius(1), fahrenheit(2) }

SensorStatus ::= TEXTUAL-CONVENTION
    STATUS current
    DESCRIPTION "The values for SensorStatus"
    SYNTAX      INTEGER { notpresent(1), ok(2), warning(3), critical(4), uploading(5), activating(6) }

OutputAccType ::= TEXTUAL-CONVENTION
    STATUS current
    DESCRIPTION "Type of Output Device"
    SYNTAX      INTEGER { lightbar(1) }

OutputAccState ::= TEXTUAL-CONVENTION
    STATUS current
    DESCRIPTION "State of accessory"
    SYNTAX      INTEGER { off(1), on(2), auto(3) }

