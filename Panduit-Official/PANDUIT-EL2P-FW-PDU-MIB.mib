--
-- Notifications
--
panduitCmnAlarmTrapsPrefix                  OBJECT IDENTIFIER       ::= { panduitCmnAlarmTraps 0 }

panduitCmnTrapAlarmEntryAdded NOTIFICATION-TYPE
    OBJECTS {
        panduitCmnAlarmId,
        panduitCmnAlarmEntity,
        panduitCmnAlarmDescr,
        panduitCmnAlarmText,
        panduitCmnAlarmSeverity }
    STATUS current
    DESCRIPTION "This trap is sent each time an alarm is inserted into the alarm table"
    ::= { panduitCmnAlarmTrapsPrefix 1 }

panduitCmnTrapAlarmEntryRemoved NOTIFICATION-TYPE
    OBJECTS {
        panduitCmnAlarmId,
        panduitCmnAlarmEntity,
        panduitCmnAlarmDescr,
        panduitCmnAlarmText,
        panduitCmnAlarmSeverity }
    STATUS current
    DESCRIPTION "This trap is sent each time an alarm is removed from the alarm table"
    ::= { panduitCmnAlarmTrapsPrefix 2 }

panduitCmnTrapEvent NOTIFICATION-TYPE
    OBJECTS {
        panduitCmnAlarmEntity,
        panduitCmnAlarmDescr,
        panduitCmnAlarmText,
        panduitCmnAlarmSeverity }
    STATUS current
    DESCRIPTION "This trap is sent when an event occurs. No alarm is entered in the table
        as the event is not a persistent condition"
    ::= { panduitCmnAlarmTrapsPrefix 3 }



--
-- Conformance
--
panduitCmnAlarmCompliances   OBJECT IDENTIFIER ::= { panduitCmnAlarmConformance 1 }
panduitCmnAlarmGroups        OBJECT IDENTIFIER ::= { panduitCmnAlarmConformance 2 }

panduitCmnAlarmFullCompliance MODULE-COMPLIANCE
    STATUS current
    DESCRIPTION "Compilance statement"
    MODULE
        MANDATORY-GROUPS {
            panduitCmnAlarmGroup,
            panduitCmnAlarmTrapGroup }
    ::= { panduitCmnAlarmCompliances 1 }

panduitCmnAlarmGroup OBJECT-GROUP
    OBJECTS {
        panduitCmnAlarmId,
        panduitCmnAlarmCount,
        panduitCmnAlarmSeveritySummary,
        panduitCmnAlarmEntity,
        panduitCmnAlarmDescr,
        panduitCmnAlarmSeverity,
        panduitCmnAlarmText,
        panduitCmnAlarmTime
    }
    STATUS current
    DESCRIPTION
        "the panduitCmnAlarmGroup identifies objects which aid in observing
        the alarms on the system"
    ::= { panduitCmnAlarmGroups 1 }

panduitCmnAlarmTrapGroup NOTIFICATION-GROUP
    NOTIFICATIONS {
        panduitCmnTrapAlarmEntryAdded,
        panduitCmnTrapAlarmEntryRemoved,
        panduitCmnTrapEvent
    }
    STATUS current
    DESCRIPTION
        "The panduitCmnpTrapGroup identifies notifications which are supported"
    ::= { panduitCmnAlarmGroups 2}
