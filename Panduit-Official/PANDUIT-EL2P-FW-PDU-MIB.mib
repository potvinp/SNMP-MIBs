--
-- Textual Conventions
--
PinMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Setting if the user pins are visible or hidden."
    SYNTAX INTEGER { visible(1), hidden (2) }

AisleControl ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Represents the aisle control function.
         standalone each aisle will be controlled separately,
         combined each aisle will be controlled together."
    SYNTAX INTEGER { combined(1), standalone(2) }

BeaconFunction ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Represents function of the beacon. Locate flash the beacon color.  Standy by will be solid
                 or blink red is a critical alarm is present or blink yellow is a warning alarm is present."
    SYNTAX INTEGER { standby(1), locate (2) }

BeaconColor ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Represents of the colors of the beacon"
    SYNTAX      INTEGER { beaconoff(1), red(2), green(3), blue(4), yellow(5), magenta(6), aqua(7), white(8) }

LockStatus ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Represents the status of the lock"
    SYNTAX      INTEGER { unlock(1), lock(2) }

AisleLocation ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Represents the location side of the cabinet."
    SYNTAX      INTEGER { cold(1), hot(2), both(3) }

AisleSwitch ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Represents the aisle switch on the handle to describe where the handle is physically located."
    SYNTAX      INTEGER { cold(1), hot(2) }

